nst data = items.map(item => item.json); // items é o array de entradas do node anterior\n\n// Função para capitalizar nomes\nfunction capitalizeName(fullName) {\n    if (!fullName) {\n        return { fname: '', lname: '', fullname: '' };\n    }\n\n    const lowerCaseWords = ['da', 'de', 'do', 'di', 'du'];\n    let words = fullName.toLowerCase().split(' ').map((word, index) => {\n        if (lowerCaseWords.includes(word) && index !== 0) {\n            return word;\n        }\n        return word.charAt(0).toUpperCase() + word.slice(1);\n    });\n\n    let fname = words.shift();\n    let lname = words.join(' ');\n\n    return { fname, lname, fullname: fname + ' ' + lname };\n}\n\n// Função para formatar email\nfunction formatEmail(email) {\n    return email.toLowerCase().trim();\n}\n\n// Processamento de cada linha do Google Sheets\nconst results = data.map(row => {\n    const fullNameAnswer = row[\"NOME\"] || \"\";\n    const emailAnswer = row[\"EMAIL\"] || \"\";\n    const telefoneClienteOriginal = row[\"WHATSAPP\"] || \"\";\n\n    // Garantir que telefoneClienteOriginal seja uma string antes de usar replace\n    const telefoneLimpo = String(telefoneClienteOriginal).replace(/\\D/g, \"\");\n    const DDI = '55';\n    const DDD = telefoneLimpo.slice(0, 2);\n\n    // Determinar o número de telefone final\n    const telefoneFinal = `${DDI}${DDD}${telefoneLimpo.slice(-9)}`;\n\n    // Formatar nomes e email\n    const formattedNames = capitalizeName(fullNameAnswer);\n    const formattedEmail = formatEmail(emailAnswer);\n    const utm_source = \"direct2\";\n    const utm_medium = \"organico\";\n    const utm_campaign = \"direct2\";\n    const utm_id = \"APA\";\n    const utm_content = \"direct2\";\n    const utm_term = \"direct2\"\n\n    // Montar o resultado para cada linha\n    return {\n        json: {\n            ...formattedNames, // Nome formatado\n            email: formattedEmail, // Email formatado\n            whatsapp: telefoneFinal, // WhatsApp formatado\n            utm_source: utm_source,\n            utm_medium: utm_medium,\n            utm_campaign: utm_campaign,\n            utm_term: utm_term,\n            utm_content: utm_content,\n            utm_id: utm_id\n\n        }\n    };\n});\n\n// Retornar os resultados processados para o próximo node no workflow\nreturn results;\n"},"id":"c1309e49-1b82-48c7-85ca-30aa3a1e3275","name":"formata_dados","type":"n8n-nodes-base.code","typeVersion":2,"position":[-880,-40]},{"parameters":{"queue":"=[L070824] - Opt in","options":{"arguments":{"argument":[{"key":"x-queue-type","value":"quorum"}]},"durable":true}},"id":"9199d0d2-2e20-442c-a57f-cef352d337e6","name":"entra_na_fila","type":"n8n-nodes-base.rabbitmq","typeVersion":1,"position":[600,-60]},{"parameters":{"assignments":{"assignments":[{"id":"6391350e-b1f4-4654-996b-edf76765c753","name":"fname","value":"={{ $node[\"formata_dados\"].json[\"fname\"] }}","type":"string"},{"id":"48614d4e-5351-4522-9047-d67280e26180","name":"lname","value":"={{ $node[\"formata_dados\"].json[\"lname\"] }}","type":"string"},{"id":"1030c484-cfab-4418-b24b-453e7089594e","name":"fullname","value":"={{ $node[\"formata_dados\"].json[\"fullname\"] }}","type":"string"},{"id":"01ae3ba3-8aab-4f9e-aeed-2a1977183633","name":"email","value":"={{ $node[\"formata_dados\"].json[\"email\"] }}","type":"string"},{"id":"2a63556c-f48a-4596-8e0d-bf7321e5ea09","name":"whatsapp","value":"={{ $node[\"formata_dados\"].json[\"whatsapp\"] }}","type":"string"},{"id":"2dc58ea8-d7b0-4c00-8ec2-f369464cbd0a","name":"utm_source","value":"={{ $node[\"formata_dados\"].json[\"utm_source\"] }}","type":"string"},{"id":"d9360580-f83e-4a7c-b475-8e0720527512","name":"=utm_medium","value":"={{ $node[\"formata_dados\"].json[\"utm_medium\"] }}","type":"string"},{"id":"84156413-eaf6-4e39-b69d-6ba4b452fbe4","name":"utm_campaign","value":"={{ $node[\"formata_dados\"].json[\"utm_campaign\"] }}","type":"string"},{"id":"4304338c-e5f2-44aa-b152-7be73e8232b1","name":"utm_term","value":"={{ $node[\